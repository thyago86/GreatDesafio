
@{
    ViewData["Title"] = "DetalhesUsuario";
    var detalhes = ViewData["Usuario"] as Usuario;
}

<h1>Detalhes deste Usuario</h1>
<p>Nome: @detalhes.Nome</p>
<p>CPF: @detalhes.Cpf</p>
<p>RG: @detalhes.Rg</p>
<p>Nome da Mãe: @detalhes.NomeMae</p>
<p>Nome do Pai: @detalhes.NomePai</p>
<p>Data de Nascimento: @detalhes.DataNascimento</p>
<p>Data de Cadastro: @detalhes.DataCadastro</p>


